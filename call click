		WHEN length($($total_cash))>=45 AND length($($total_cash)) < 		49 THEN 		 SET $title = 			('Nonillionaire');	
		WHEN length($($total_cash))>=41 AND length($($total_cash)) < 		45 THEN 		 SET $title = 			('Octillionaire');	
		WHEN length($($total_cash))>=37 AND length($($total_cash)) < 		41 THEN 		 SET $title = 			('Septillionaire');	
		WHEN length($($total_cash))>=33 AND length($($total_cash)) < 		37 THEN 		 SET $title = 			('Sextillionaire');	
		WHEN length($($total_cash))>=29 AND length($($total_cash)) < 		33 THEN 		 SET $title = 			('Quintillionaire');	
		WHEN length($($total_cash))>=25 AND length($($total_cash)) < 		29 THEN 		 SET $title = 			('Quadrillionaire');	
		WHEN length($($total_cash))>=21 AND length($($total_cash)) < 		25 THEN 		 SET $title = 			('Trillionaire');	
		WHEN length($($total_cash))>=17 AND length($($total_cash)) < 		21 THEN 		 SET $title = 			('Billionaire');	
		WHEN length($($total_cash))>=13 AND length($($total_cash)) < 		17 THEN 		 SET $title = 			('Millionaire');	
        WHEN length($($total_cash))<13 							       THEN 		 SET $title = (' ');



	else begin end; end case;
      

	SET $current_level = ( select level from click where active=1),
		$current_mult = ( select mult from click where active=1);
		-- UI				
select  $title as'',$($total_cash) as cash, concat('$', format($current_mult,2)) as CPS, $clicks as clicks, $current_level as level, $system as message;

 END //
 delimiter ;
 
 
